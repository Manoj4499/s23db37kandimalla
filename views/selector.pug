extends layout

block content
  h1= title
  - let image_names = ["awm.jpg","m416.jpg","pistel.jpg","shot gun.jpg","hk416.jpg"]
  - for(let i=0;i<3;i++){
      - let imgs = Math.floor(Math.random()*5);
      img(src=`/images/${image_names[imgs]}` alt=`${image_names}` style+'height:300px; width:300px')
  - }